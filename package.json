{
  "name": "novahub-obfuscator-service",
  "version": "1.0.0",
  "description": "NovaHub Obfuscator (API + Discord bot)",
  "main": "server.js",
  "scripts": {
    "start": "concurrently \"npm run start:web\" \"npm run start:bot\" \"npm run start:bypass\"",
    "start:web": "node server.js",
    "start:bot": "node bot.js",
    "start:bypass": "node bypasser.js",

    "dev": "concurrently \"npm run dev:web\" \"npm run dev:bot\" \"npm run dev:bypass\"",
    "dev:web": "nodemon server.js",
    "dev:bot": "nodemon bot.js",
    "dev:bypass": "nodemon bypasser.js"
  },
  "author": "NovaHub",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.0",
    "discord.js": "^14.14.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
